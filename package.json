{
  "name": "bem-site-engine",
  "description": "BEM CMS based on Express framework",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/bem/bem-site-engine.git"
  },
  "engines": {
    "node": ">=0.8"
  },
  "dependencies": {
    "express": "3.4.7",
    "connect-slashes": "~1.2.0",
    "terror": "0.3.4",
    "json-stringify-safe": "5.0.0",
    "cron": "1.0.1",
    "request": "2.33.0",
    "mime": "1.2.11",
    "fs-extra": "~0.8.1",
    "susanin": "~0.1.11",
    "nconf": "~0.6.9",
    "vow": "~0.4.x",
    "vow-fs": "0.3.0",
    "lodash": "~2.4.1",
    "intel": "1.0.0-b2",
    "sha1": "~1.1.0",
    "marked": "*",
    "semver": "~2.3.0",
    "js2xmlparser": "*",
    "github": "*",
    "yandex-disk": "0.0.6",
    "coa": "~0.4.0",
    "deep-extend": "~0.2.10",
    "js-beautify": "1.5.1"
  },
  "devDependencies": {
    "borschik": "~1.0.5",
    "bower": "~1.3.9",
    "bower-npm-install": "~0.5.8",
    "enb": "~0.13.4",
    "enb-autoprefixer": "~0.1.1",
    "enb-bembundle": "~1.0.2",
    "enb-bemxjst": "~1.2.0",
    "enb-borschik": "~1.0.4",
    "enb-diverse-js": "*",
    "enb-modules": "*",
    "enb-stylus": "^1.1.4",
    "inherit": "~2.2.2",
    "jscs": "~1.5.9",
    "jshint": "~2.5.5"
  },
  "scripts": {
    "start": "node bin/app.js",
    "make": "npm install && node postinstall.js && node bin/data.js && npm start",
    "clean": "enb make clean && rm -rf node_modules && rm -rf libs && rm -rf cache && rm -rf backups && rm -rf logs",
    "clean_logs": "rm -rf logs/*",
    "clean_cache": "rm -rf cache/*",
    "clean_data": "rm -rf backups/*",
    "test": "node_modules/.bin/jshint . && node_modules/.bin/jscs -c .jscs.js ."
  },
  "author": "info@bem.info",
  "readmeFilename": "README.md",
  "private": true
}
