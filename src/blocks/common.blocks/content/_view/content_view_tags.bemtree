block('content').mod('view', 'tags').content()(function () {
    return [
        applyNext(),
        {
            block: this.block,
            elem: 'tag-posts',
            content: this.data.posts.map(function (node) {
                return {
                    block: 'posts',
                    title: node.title,
                    node: node
                };
            })
        }
    ];
});
