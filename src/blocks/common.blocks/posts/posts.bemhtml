block('posts')(

    def().match(function() {
        var c = this.ctx.content;
        return !c || !c.length;
    })(function() {
        return false;
    }),

    content().match(this.ctx.title)(function(){
        return [
            {
                elem: 'title',
                content: this.ctx.title
            },
            applyNext()
        ];
    })
);
